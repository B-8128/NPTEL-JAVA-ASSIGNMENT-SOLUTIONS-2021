PROGRAMMING ASSIGNMENT--3
//Read any two values for a and b.

//Get the result of a/b;
a=input.nextInt();
b=input.nextInt();
try
{
  System.out.print(a/b);
}
catch(ArithmeticException e)
{
 System.out.print("Exception caught: Division by zero."); 
}

