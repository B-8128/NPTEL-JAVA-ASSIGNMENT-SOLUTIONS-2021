PROGRAMMING ASSIGNMENT----4
// Returns the sum of a and b. (reader)
// Should always return an even number.
synchronized public int sum() {
return(a+b);
}

// Increments both a and b. (writer)
synchronized public void inc() {
++a;
++b;
}
}

